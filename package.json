{
  "name": "eshoptrial",
  "version": "0.0.1",
  "description": "Sample project that uses Material-UI",
  "scripts": {
    "start": "webpack-dev-server --config webpack-dev-server.config.js --progress --inline --colors",
    "start-prod": "webpack-dev-server --config webpack-production.config.js --progress --inline --colors",
    "build": "npm run test && npm run prebuild && ./node_modules/.bin/webpack --config webpack-production.config.js --progress --colors",
    "prebuild": "rm -rf build && mkdir build",
    "test": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha --require test/helpers/setup.js -w test/helpers/browser.js 'test/**/*.spec.js'"
  },
  "private": true,
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "html-webpack-plugin": "^2.24.1",
    "transfer-webpack-plugin": "^0.1.4",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "babel-preset-airbnb": "^2.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-enzyme": "^0.6.1",
    "css-loader": "^0.26.1",
    "css-modules-require-hook": "^4.0.5",
    "enzyme": "^2.6.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "istanbul": "^1.0.0-alpha",
    "jsdom": "^9.8.3",
    "material-ui": "^0.16.4",
    "mocha": "^3.2.0",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0",
    "react-tap-event-plugin": "^2.0.1",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7"
  },
  "main": "index.js",
  "license": "MIT"
}
